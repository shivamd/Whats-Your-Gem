{
  "manifest_version": 2,
    "name": "Whats your gem?",
    "description": "Gem annotations on Github in Gemfiles.",
    "version": "1.1.0",
    "permissions": [
      "https://github.com/",
      "https://rubygems.org/"
    ],
    "browser_action": {
      "default_icon": "icon.png"
    },
    "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-2.1.1.min.js","content.js", "style.js"]
    }
  ]
}
